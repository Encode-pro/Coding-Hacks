<?xml version="1.0" encoding="utf-8"?>



<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#bbdefb">



<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".QuestionsActivity"
    android:padding="16dp">
<TextView
    android:id="@+id/Que1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="#000000"
    android:text="Q1.Where to place f with a double constant 3.14 to specify it as a float ? A - (f)(3.14) B -  3.14f C - f(3.14)" />

    <EditText
        android:id="@+id/Ans1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que1"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B or C"/>


    <TextView
        android:id="@+id/Que2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans1"
        android:text="Q2. push() and pop() functions are found in A - queues B - lists C - stacks D - trees" />
    <EditText
        android:id="@+id/Ans2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que2"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>


    <TextView
        android:id="@+id/Que3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans2"
        android:text="Q3. Linker generates ___ file.

A - Object code

B - Executable code

C - Assembly code

D - None of the above."    />
    <EditText
        android:id="@+id/Ans3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que3"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>


    <TextView
        android:id="@+id/Que4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans3"
        android:text="Q4. Choose the respective delete operator usage for the expression ‘ptr=new int[100]’.

A - delete ptr;

B - delete ptr[];

C - delete[] ptr;

D - []delete ptr;"    />
    <EditText
        android:id="@+id/Ans4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que4"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>



    <TextView
        android:id="@+id/Que5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans4"
        android:text="Q5. How can we generate random numbers in python using methods?

A - random.uniform ()

B - random.randint()

C - random.random()

D - All of the above"    />
    <EditText
        android:id="@+id/Ans5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que5"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>



    <TextView
        android:id="@+id/Que6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans5"
        android:text="Q6. Runtime polymorphism is done using.

A - Function overloading

B - Virtual classes

C - Virtual functions

D - Friend function"    />
    <EditText
        android:id="@+id/Ans6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que6"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>



    <TextView
        android:id="@+id/Que7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans6"
        android:text="Q7. How many pointers are there in Java? A - One B - Two C - Zero D - Infinite"
        />

    <EditText
        android:id="@+id/Ans7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que7"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>



    <TextView
        android:id="@+id/Que8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans7"
        android:text="Q8. Which of following is used to handle null exceptions in Kotlin?

A - Range
B -  Sealed Class
C -  Elvis Operator
D -  Lambda function"/>


    <EditText
        android:id="@+id/Ans8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que8"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>


    <TextView
        android:id="@+id/Que9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans8"
        android:text="Q9. Which file extension is used to save Kotlin files.

A -  .java
B -  .kot
C -  .kt or .kts
D -  .andriod"/>


    <EditText
        android:id="@+id/Ans9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que9"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>

    <TextView
        android:id="@+id/Que10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/Ans9"
        android:text="Q10. ___ is used to find and fix bugs in the Java programs.

A - JVM
B - JRE
C - JDK
D - JDB"/>


    <EditText
        android:id="@+id/Ans10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Que10"
        android:background="#1e88e5"
        android:lines="1"
        android:padding="8dp"
        android:hint="Enter A,B,C or D"/>




    <Button
    android:id="@+id/btnSubmit"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
        android:layout_below="@+id/Ans10"
    android:text="Submit"

    android:layout_centerHorizontal="true"
    android:backgroundTint="#000000"/>

</RelativeLayout>

</ScrollView>


